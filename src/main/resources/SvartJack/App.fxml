<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.control.Label?>

<AnchorPane fx:id="background" prefHeight="900.0" prefWidth="1440.0"
            xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="SvartJack.SvartJackController">

    <!-- Dealer-boks Ã¸verst midt -->
    <Pane fx:id="dealer" prefWidth="200" prefHeight="150" layoutX="620" layoutY="50">
        <children>
            <Label text="Dealer" layoutX="10" layoutY="10"/>
        </children>
    </Pane>

    <!-- Seksjon for spillere nederst -->
    <HBox fx:id="players" spacing="50" AnchorPane.leftAnchor="0" AnchorPane.rightAnchor="0" layoutY="700">
        <children>
            <Pane fx:id="player1">
                <children><Label text="Player 1" layoutX="10" layoutY="10"/></children>
            </Pane>
            <Pane fx:id="player2">
                <children><Label text="Player 2" layoutX="10" layoutY="10"/></children>
            </Pane>
            <Pane fx:id="player3">
                <children><Label text="Player 3" layoutX="10" layoutY="10"/></children>
            </Pane>
            <Pane fx:id="player4">
                <children><Label text="Player 4" layoutX="10" layoutY="10"/></children>
            </Pane>
            <Pane fx:id="player5">
                <children><Label text="Player 5" layoutX="10" layoutY="10"/></children>
            </Pane>
        </children>
    </HBox>

</AnchorPane>